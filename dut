S_000001dc40b4e540 .scope module, "tb_melay" "tb_melay" 2 5;
 .timescale -9 -12;
v000001dc40a13600_0 .var "clk", 0 0;
v000001dc40a136a0_0 .var "in", 0 0;
v000001dc40a13740_0 .net "out", 0 0, v000001dc40b4bf40_0;  1 drivers
v000001dc40a137e0_0 .var "rst", 0 0;
S_000001dc40b4e6d0 .scope module, "dut" "melay" 2 9, 3 3 0, S_000001dc40b4e540;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "in";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /OUTPUT 1 "out";
P_000001dc40b47570 .param/l "ST_one" 0 3 10, C4<1>;
P_000001dc40b475a8 .param/l "ST_zero" 0 3 9, C4<0>;
v000001dc40b47330_0 .net "clk", 0 0, v000001dc40a13600_0;  1 drivers
v000001dc40b47080_0 .net "in", 0 0, v000001dc40a136a0_0;  1 drivers
v000001dc40b4bea0_0 .var "next_state", 0 0;
v000001dc40b4bf40_0 .var "out", 0 0;
v000001dc40b4e860_0 .net "rst", 0 0, v000001dc40a137e0_0;  1 drivers
v000001dc40b4e900_0 .var "state", 0 0;
E_000001dc40a1a430 .event anyedge, v000001dc40b4e900_0, v000001dc40b47080_0;
E_000001dc40a1b070 .event posedge, v000001dc40b47330_0;
    .scope S_000001dc40b4e6d0;
T_0 ;
    %wait E_000001dc40a1b070;
    %load/vec4 v000001dc40b4e860_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001dc40b4e900_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v000001dc40b4bea0_0;
    %assign/vec4 v000001dc40b4e900_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_000001dc40b4e6d0;
T_1 ;
    %wait E_000001dc40a1a430;
    %load/vec4 v000001dc40b4e900_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %jmp T_1.2;
T_1.0 ;
    %load/vec4 v000001dc40b47080_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.3, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v000001dc40b4bf40_0, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001dc40b4bea0_0, 0, 1;
    %jmp T_1.4;
T_1.3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001dc40b4bf40_0, 0;
    %load/vec4 v000001dc40b4e900_0;
    %store/vec4 v000001dc40b4bea0_0, 0, 1;
T_1.4 ;
    %jmp T_1.2;
T_1.1 ;
    %load/vec4 v000001dc40b47080_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.5, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v000001dc40b4bf40_0, 0;
    %load/vec4 v000001dc40b4e900_0;
    %store/vec4 v000001dc40b4bea0_0, 0, 1;
    %jmp T_1.6;
T_1.5 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001dc40b4bf40_0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001dc40b4bea0_0, 0, 1;
T_1.6 ;
    %jmp T_1.2;
T_1.2 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_000001dc40b4e540;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001dc40a13600_0, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v000001dc40a13600_0, 0;
    %delay 10000, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_000001dc40b4e540;
T_3 ;
    %vpi_call 2 19 "$dumpfile", "melay.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v000001dc40a137e0_0, 0;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001dc40a137e0_0, 0;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001dc40a136a0_0, 0;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v000001dc40a136a0_0, 0;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001dc40a136a0_0, 0;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v000001dc40a136a0_0, 0;
    %delay 20000, 0;
